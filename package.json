{
  "name": "Artefact-Labs",
  "version": "0.0.1",
  "private": true,
  "description": "The Official web site of Artefact-Labs",
  "author": "Artefact-Team",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "pnpm run generate:pwa-icon & vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "capacitor:run:android": "pnpm run build && npx cap run android",
    "capacitor:run:ios": "pnpm run build && npx cap run ios",
    "electron:build": "pnpm run generate-ico & vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "generate:icon": "pnpm run generate:pwa-icon & generate-ico",
    "generate-ico": "png-to-ico ./src/assets/icon.png > ./src/assets/icon.ico",
    "generate:pwa-icon": "npx pwa-asset-generator src/assets/icon.png public/img --opaque false --icon-only --favicon --type png --padding 0",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@capacitor/android": "^3.2.3",
    "@capacitor/cli": "^3.2.3",
    "@capacitor/core": "^3.2.3",
    "@capacitor/ios": "^3.2.3",
    "@mdi/font": "^3.6.95",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "artefact-controller": "0.0.5",
    "balloon-css": "^1.0.4",
    "core-js": "^3.15.2",
    "file-saver": "^2.0.5",
    "ionicons": "4.6.3",
    "jquery": "^3.6.0",
    "material-design-icons-iconfont": "^5.0.1",
    "phaser": "^3.55.2",
    "pwa-asset-generator": "^3.2.2",
    "register-service-worker": "^1.7.2",
    "roboto-fontface": "*",
    "typed.js": "^2.0.12",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-codemirror": "^4.0.6",
    "vue-i18n": "^8.25.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.5.2",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.5.6",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/test-utils": "^1.1.3",
    "electron": "^13.1.7",
    "electron-devtools-installer": "^3.2.0",
    "graphql-tag": "^2.9.0",
    "node-sass": "^4.12.0",
    "png-to-ico": "^2.1.2",
    "sass": "^1.35.2",
    "sass-loader": "^8.0.2",
    "spectron": "^11.0.0",
    "vue-cli-plugin-apollo": "~0.22.2",
    "vue-cli-plugin-cordova": "~2.4.1",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.6",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}